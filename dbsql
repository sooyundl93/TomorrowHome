--system 사용자로 접속해서 아래 명령어들을 실행한다.
--사용자 생성 및 권한 부여
CREATE USER tmr_db
IDENTIFIED BY 1234
DEFAULT TABLESPACE USERS
TEMPORARY TABLESPACE TEMP;
​
GRANT CONNECT TO tmr_db;
GRANT RESOURCE TO tmr_db;
GRANT DBA TO tmr_db;
​
--tmr_db 데이터베이스(사용자) 생성 완료.
COMMIT;


-- #####################################################################################
-- ################################# 테이블 CREATE######################################
-- #####################################################################################

-- ############################# 사용자 정보 테이블 CREATE #############################
CREATE TABLE member_info (
    user_id VARCHAR2(40) PRIMARY KEY, --사용자 식별자
    user_password VARCHAR2(256) NOT NULL, --비밀번호 
    user_name VARCHAR2(20) NOT NULL, --이름
    birthday DATE NOT NULL, --생년월일
    email VARCHAR2(50) NOT NULL, --이메일
    receive_email_flag CHAR NOT NULL, --이메일 수신여부
    phone_number VARCHAR2(11) NOT NULL, --연락처
    receive_messages_flag CHAR NOT NULL, --문자 수신여부
    postal_code NUMBER(5) NOT NULL, --우편번호
    address VARCHAR2(50) NOT NULL, --주소
    detailed_address VARCHAR2(50) NOT NULL, --상세주소
    pw_question NUMBER(2), --비밀번호 질문
    pw_answer VARCHAR2(200), --비밀번호 질문 답
    member_since DATE DEFAULT SYSDATE, --가입일
    user_role VARCHAR2(20) DEFAULT 'ROLE_TEMPORARY_USER', --사용자 권한
    mileage NUMBER (10) DEFAULT 0, --보유한 마일리지
    number_of_visits NUMBER(8) DEFAULT 0, --일일 방문 횟수
    
    CONSTRAINT pw_question_check CHECK(pw_question BETWEEN 1 AND 10),
    CONSTRAINT receive_email_flag_check CHECK(receive_email_flag IN('Y', 'N')),
    CONSTRAINT receive_messages_flag_check CHECK(receive_messages_flag IN('Y', 'N')),
    CONSTRAINT manager_flag_check CHECK(manager_flag IN('ROLE_ADMIN', 'ROLE_USER', 'ROLE_TEMPORARY_USER'))
);

-- ############################# 상품 정보 테이블 ###############################
CREATE TABLE product_info (
    product_code CHAR(6) PRIMARY KEY, --상품 식별코드
    product_name VARCHAR2(100) NOT NULL, --상품이름
    parent_category VARCHAR2(30) NOT NULL, --상위 카테고리
    child_category VARCHAR2(30) NOT NULL, --하위 카테고리
    added_date DATE DEFAULT SYSDATE, --상품이 추가된 날짜
    manufacturer VARCHAR2(30) NOT NULL, --제조사
    product_price NUMBER(10) NOT NULL, --상품가격
    product_description VARCHAR2(3000), --상품설명
    number_of_sales NUMBER(8) DEFAULT 0, --판매된 수량
    
    CONSTRAINT parent_category_check 
        CHECK(parent_category 
        IN('거실', '주방', '침실', '서재', '인테리어', '미분류')),
    CONSTRAINT child_category_check 
        CHECK(child_category 
        IN('쇼파', '테이블', '의자', '수납장', '침대', '화장대', '책상', '책장', '미분류'))
);

-- ############################# 장바구니 테이블 ################################
CREATE TABLE shopping_cart (
    item_no NUMBER(8) PRIMARY KEY, --장바구니 품목 식별번호
    user_id VARCHAR2(40) NOT NULL, --사용자 식별자
    product_code CHAR(6) NOT NULL, --상품 식별코드
    number_of_products NUMBER(2) NOT NULL, --담은 개수
    
    CONSTRAINT shopping_user_id_fk FOREIGN KEY(user_id)
        REFERENCES member_info(user_id) ON DELETE CASCADE,
    CONSTRAINT shopping_product_code_fk FOREIGN KEY(product_code)
        REFERENCES product_info(product_code) ON DELETE CASCADE
);

-- ############################# 주문 정보 테이블 ################################
CREATE TABLE order_info (
    order_id CHAR(11) PRIMARY KEY, --주문 정보 식별자(날마다 시퀸스 초기화하는 프로시저 필요)
    user_id VARCHAR2(40) NOT NULL, --사용자 식별자
    order_date DATE DEFAULT SYSDATE, --주문날짜
    recipient_name VARCHAR2(20) NOT NULL, --수령인
    phone_number VARCHAR2(11) NOT NULL, --연락처
    postal_code NUMBER(5) NOT NULL, --우편번호
    address VARCHAR2(50) NOT NULL, --주소
    detailed_address VARCHAR2(50) NOT NULL, --상세주소
    shipping_memo VARCHAR2(200), --배송메모
    total_price NUMBER(10) NOT NULL, --총 금액
    used_mileage NUMBER(10) NOT NULL, --사용한 마일리지 
    discounted_price NUMBER(10) NOT NULL, --할인된 금액
    shipping_cost NUMBER(10) NOT NULL, --배송비용
    payment_method NUMBER(2) NOT NULL, --결제수단
    payment_id VARCHAR2(50), --결제 고유 번호
    accrued_mileage NUMBER(10) NOT NULL, --적립된 마일리지
    order_status NUMBER(2) DEFAULT 1, --주문상태
    wrote_review_flag CHAR DEFAULT 'N', --리뷰작성여부
    
    CONSTRAINT discounted_price_check CHECK (discounted_price = total_price - used_mileage),
    CONSTRAINT payment_method_check CHECK (payment_method BETWEEN 1 AND 3),
    CONSTRAINT accrued_mileage_check CHECK (accrued_mileage = discounted_price * 0.05),
    CONSTRAINT order_status_check CHECK (order_status BETWEEN 1 AND 6),
    CONSTRAINT wrote_review_flag_check CHECK(wrote_review_flag IN('Y', 'N'))
);

-- ############################# 주문 상품 정보 테이블 ###########################
CREATE TABLE order_product_info (
    item_no NUMBER(8) PRIMARY KEY, --주문한 품목 식별번호
    order_id CHAR(11) NOT NULL, --주문 식별자
    product_code CHAR(6) NOT NULL, --상품 식별코드
    product_name VARCHAR2(100) NOT NULL, --주문한 상품이름
    parent_category VARCHAR2(30) NOT NULL, --상위 카테고리
    child_category VARCHAR2(30) NOT NULL, --하위 카테고리
    manufacturer VARCHAR2(30) NOT NULL, --제조사
    product_price NUMBER(10) NOT NULL, --주문한 상품 가격
    number_of_orders NUMBER(3) NOT NULL, --주문 개수
    
    CONSTRAINT order_id_fk FOREIGN KEY(order_id)
        REFERENCES order_info(order_id) ON DELETE CASCADE
);

-- ############################# 회원 상품 리뷰 테이블 ###########################
CREATE TABLE member_review (
    post_no NUMBER(8) PRIMARY KEY, --회원 리뷰 글 식별번호
    product_code CHAR(6) NOT NULL, --상품 식별코드
    user_id VARCHAR2(40) NOT NULL, --리뷰 쓴 사용자 식별자
    review_score NUMBER(2, 1) NOT NULL, --별점 0.0 ~ 5.0
    post_date DATE DEFAULT SYSDATE, --작성날짜
    post_content VARCHAR2(1500) NOT NULL, --리뷰내용
    
    CONSTRAINT review_user_id_pk FOREIGN KEY(user_id)
        REFERENCES member_info(user_id) ON DELETE CASCADE,
    CONSTRAINT review_score_check CHECK (review_score BETWEEN 0.5 AND 5.0)
);

-- ############################# 공지사항 테이블 ################################
CREATE TABLE notice (
    post_no NUMBER(8) PRIMARY KEY, --공지사항 글 식별번호
    user_id VARCHAR2(40) NOT NULL, --글을 쓴 관리자 식별자
    post_title VARCHAR2(50) NOT NULL, --글 제목
    post_content VARCHAR2(3000) NOT NULL, --글 내용
    post_date DATE DEFAULT SYSDATE, --작성한 날짜
    post_hits NUMBER(10) DEFAULT 0, --글 조회수
    important_flag CHAR DEFAULT 'N', --중요 공지 플래그
    
    CONSTRAINT important_flag_check CHECK (important_flag IN('Y', 'N'))
);

-- ############################# 상품 이미지 정보 테이블 #########################
CREATE TABLE product_image_info (
    image_no NUMBER(8) PRIMARY KEY, --이미지 식별번호
    product_code CHAR(6) NOT NULL, --이미지가 업로드 된 상품의 식별코드
    image_file_name VARCHAR2(300) NOT NULL, --이미지 파일 명
    
    CONSTRAINT product_no_fk FOREIGN KEY(product_code)
        REFERENCES product_info(product_code) ON DELETE CASCADE
);

-- ############################# 회원 리뷰 이미지 정보 테이블 #####################
CREATE TABLE review_image_info (
    image_no NUMBER(8) PRIMARY KEY, --이미지 식별번호
    post_no NUMBER(8) NOT NULL, --이미지가 업로드 된 글의 식별자
    image_file_name VARCHAR2(300) NOT NULL, --이미지 파일 명
    
    CONSTRAINT review_post_no_fk FOREIGN KEY(post_no)
        REFERENCES member_review(post_no) ON DELETE CASCADE
);

-- ############################# 공지사항 이미지 정보 테이블 ######################
CREATE TABLE notice_image_info (
    image_no NUMBER(8) PRIMARY KEY, --이미지 식별번호
    post_no NUMBER(8) NOT NULL, --이미지가 업로드 된 글의 식별자
    image_file_name VARCHAR2(300) NOT NULL, --이미지 파일 명
    
    CONSTRAINT notice_post_no_fk FOREIGN KEY(post_no)
        REFERENCES notice(post_no) ON DELETE CASCADE
);

-- ############################# 상품 문의 글 테이블 #############################
CREATE TABLE product_inquiry (
    inquiry_no NUMBER(8) PRIMARY KEY, --문의글 식별번호
    product_code CHAR(6), --상품 식별코드
    user_id VARCHAR2(40)NOT NULL, --글을 쓴 사용자 식별자
    inquiry_content VARCHAR2(3000) NOT NULL, --문의내용
    private_flag CHAR NOT NULL, --비공개 설정 여부
    inquiry_date DATE DEFAULT SYSDATE, --작성날짜
    number_of_replies NUMBER(2) DEFAULT 0, --답변 받은 횟수
    
    CONSTRAINT product_inquiry_user_id_fk FOREIGN KEY(user_id)
        REFERENCES member_info(user_id) ON DELETE CASCADE
);

-- ############################# 1:1 문의 글 테이블 #############################
CREATE TABLE one2one_inquiry (
    inquiry_no NUMBER(8) PRIMARY KEY, --문의글 식별번호
    user_id VARCHAR2(40) NOT NULL, --글을 쓴 사용자 식별자
    inquiry_title VARCHAR2(50) NOT NULL, --글 제목
    inquiry_content VARCHAR2(3000) NOT NULL, --문의내용
    inquiry_date DATE DEFAULT SYSDATE, --작성날짜
    number_of_replies NUMBER(2) DEFAULT 0, --답변 받은 횟수

    CONSTRAINT one2one_inquiry_user_id_fk FOREIGN KEY(user_id)
        REFERENCES member_info(user_id) ON DELETE CASCADE
);

-- ############################# 상품 문의 답글 테이블 ##########################
CREATE TABLE product_inquiry_reply (
    reply_no NUMBER(8) PRIMARY KEY, --답글 식별번호
    inquiry_no NUMBER(8) NOT NULL, --문의글 식별번호
    user_id VARCHAR2(40) NOT NULL, --글을 쓴 관리자 식별자
    reply_content VARCHAR2(3000) NOT NULL, --답변 내용
    reply_date DATE DEFAULT SYSDATE, --작성날짜
    
    CONSTRAINT product_inquiry_no_fk FOREIGN KEY(inquiry_no)
        REFERENCES product_inquiry(inquiry_no) ON DELETE CASCADE
);

-- ############################# 1:1 문의 답글 테이블 ###########################
CREATE TABLE one2one_inquiry_reply (
    reply_no NUMBER(8) PRIMARY KEY, --답글 식별번호
    inquiry_no NUMBER(8) NOT NULL, --문의글 식별번호
    user_id VARCHAR2(40) NOT NULL, --글을 쓴 관리자 식별자
    reply_content VARCHAR2(3000) NOT NULL, --답변 내용
    reply_date DATE DEFAULT SYSDATE, --작성날짜
    
    CONSTRAINT one2one_inquiry_no_fk FOREIGN KEY(inquiry_no)
        REFERENCES one2one_inquiry(inquiry_no) ON DELETE CASCADE
);




-- #####################################################################################
-- ################################# 시퀀스 CREATE######################################
-- #####################################################################################


-- $$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 상품 정보 시퀸스 $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$
CREATE SEQUENCE product_info_seq
    START WITH 1
    INCREMENT BY 1
    MAXVALUE 99999;

-- $$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 장바구니 시퀸스 $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$
CREATE SEQUENCE shopping_cart_seq
    START WITH 1
    INCREMENT BY 1
    MAXVALUE 99999999;

-- $$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 주문 정보 시퀸스 $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$
CREATE SEQUENCE order_info_seq
    START WITH 1
    INCREMENT BY 1
    MAXVALUE 999;
    
-- $$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 주문 상품 시퀸스 $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$
CREATE SEQUENCE order_product_info_seq
    START WITH 1
    INCREMENT BY 1
    MAXVALUE 99999999;

-- $$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 회원 상품 리뷰 시퀸스 $$$$$$$$$$$$$$$$$$$$$$$$$$$
CREATE SEQUENCE member_review_seq
    START WITH 1
    INCREMENT BY 1
    MAXVALUE 99999999;
    
-- $$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 공지사항 시퀸스 $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$
CREATE SEQUENCE notice_seq
    START WITH 1
    INCREMENT BY 1
    MAXVALUE 99999999;

-- $$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 상품 이미지 정보 시퀸스 $$$$$$$$$$$$$$$$$$$$$$$$$
CREATE SEQUENCE product_image_info_seq
    START WITH 1
    INCREMENT BY 1
    MAXVALUE 99999999;
    
-- $$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 회원 리뷰 이미지 정보 시퀸스 $$$$$$$$$$$$$$$$$$$$
CREATE SEQUENCE review_image_info_seq
    START WITH 1
    INCREMENT BY 1
    MAXVALUE 99999999;
    
-- $$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 공지 이미지 정보 시퀸스 $$$$$$$$$$$$$$$$$$$$$$$$$
CREATE SEQUENCE notice_image_info_seq
    START WITH 1
    INCREMENT BY 1
    MAXVALUE 99999999;

-- $$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 상품 문의 글 시퀸스 $$$$$$$$$$$$$$$$$$$$$$$$$$$$
CREATE SEQUENCE product_inquiry_seq
    START WITH 1
    INCREMENT BY 1
    MAXVALUE 99999999;
    
-- $$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 1:1 문의 글 시퀸스 $$$$$$$$$$$$$$$$$$$$$$$$$$$$$
CREATE SEQUENCE one2one_inquiry_seq
    START WITH 1
    INCREMENT BY 1
    MAXVALUE 99999999;
    
-- $$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 문의 답변 글 시퀸스 $$$$$$$$$$$$$$$$$$$$$$$$$$$$
CREATE SEQUENCE product_inquiry_reply_seq
    START WITH 1
    INCREMENT BY 1
    MAXVALUE 99999999;
    
-- $$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 문의 답변 글 시퀸스 $$$$$$$$$$$$$$$$$$$$$$$$$$$$
CREATE SEQUENCE one2one_inquiry_reply_seq
    START WITH 1
    INCREMENT BY 1
    MAXVALUE 99999999;
    
    


