-- 공지사항 중요 플래그 변경
UPDATE notice
    SET important_flag = 'Y'
    WHERE post_no = 1
       OR post_no = 5
       OR post_no = 15
       OR post_no = 17;

UPDATE member_info
    SET mileage = '10000'
    WHERE user_id = 'jiae0521'
       OR user_id = 'joonYeop'
       OR user_id = 'gildong2';
       
UPDATE member_info
    SET mileage = '5000'
    WHERE user_id = 'flowerdeer'
       OR user_id = 'queenyuna'
       OR user_id = 'hmson7';
       

